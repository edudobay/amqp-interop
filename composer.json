{
  "name": "queue-interop/amqp-interop",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "repositories": [
    { "type": "git", "url": "https://github.com/edudobay/queue-interop.git" }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0",
    "queue-interop/queue-interop": "dev-php8@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.5 || ^9.3"
  },
  "autoload": {
    "psr-4": {
      "Interop\\Amqp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Interop\\Amqp\\Tests\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
